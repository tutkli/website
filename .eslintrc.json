{
    "env": {
        "node": true,
        "browser": true,
        "es2022": true
    },
    "plugins": [
        "@typescript-eslint",
        "prettier",
        "solid",
        "jsx-a11y"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:solid/recommended",
        "plugin:jsx-a11y/recommended",
        "plugin:prettier/recommended"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    }
}
